page Subscriptions {
    tabs {
        "All" => table.Users,
        "Paid" => table.Users(status="paid"),
        "Dued" => table.Users(status="dued"),
    }
}

@get("/subscriptions")
@paginate(size=10, infinite=false)
table Subscriptions {
    args {
        status: str
    }

    column {
        id: str
        tier: str
        status: str
        price: str
        created_at: str
    }
}

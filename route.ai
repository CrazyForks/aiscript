route /poc {
    get /hello {
        query {
            @string(min_len=3, max_len=10)
            content: str
            aget: int = 18
        }

        return "hello", 200
    }

    post /hello {
        query {
            @string(min_len=3, max_len=10)
            content: str
            aget: int = 18
        }

        @json
        body {
            test: bool = true
        }

        return "hello", 200
    }
}
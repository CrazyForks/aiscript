print 3 ** 3;    // expect: 27
print 2 ** 4;    // expect: 16
print 5 ** 2;    // expect: 25
print 5 ** 0;    // expect: 1
print 0 ** 0;    // expect: 1
print 0 ** 1;    // expect: 0
print 2 ** -1;   // expect: 0.5
print 2 ** -0.5; // expect: 0.7071067811865476
print 2 ** -0;   // expect: 1
print -1 ** 2;   // expect: 1
print -0.5 ** 2;   // expect: 0.25
print -0.5 ** 3;   // expect: -0.125
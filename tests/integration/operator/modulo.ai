print(5 % 2);  // expect: 1
print(5 % 2 * 2);  // expect: 2
print(5 % 2 / 1);  // expect: 1
print(10 % 3 - 1); // expect: 0
print(7 % 4 + 1);  // expect: 4
print(2 + 3 % 2);  // expect: 3
print(2 + (3 % 2));  // expect: 3
print((2 + 3) % 2);  // expect: 1
print(10 % 2); // expect: 0
print(10.0 % 2); // expect: 0
print(10.0 % 2.0); // expect: 0
print(2 % 0); // expect: NaN

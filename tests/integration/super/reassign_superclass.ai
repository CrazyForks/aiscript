class Base {
  fn method() {
    print "Base.method()";
  }
}

class Derived(Base) {
  fn method() {
    super.method();
  }
}

class OtherBase {
  fn method() {
    print "OtherBase.method()";
  }
}

let derived = Derived();
derived.method(); // expect: Base.method()
Base = OtherBase;
derived.method(); // expect: Base.method()

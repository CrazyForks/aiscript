fn match_string(s) {
    return match s {
        "hello" | "hi" => "greeting",
        "bye" => "farewell",
        s if s.starts_with("#") => "sharp",
        x if x.starts_with("@") => "at",
        x if x in ["luck", "magic"] => "lucky",
        s if s == "ai" => "ai",
        _ => "unknown",
    };
}

print(match_string("hi"));      // expect: greeting
print(match_string("bye"));     // expect: farewell
print(match_string("#id"));     // expect: sharp
print(match_string("@email"));  // expect: at
print(match_string("luck"));    // expect: lucky
print(match_string("ai"));      // expect: ai
print(match_string("match"));   // expect: unknown

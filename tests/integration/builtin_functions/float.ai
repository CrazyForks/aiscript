print(float(1));       // expect: 1
print(float(0));       // expect: 0
print(float(-1));      // expect: -1
print(float("1.5"));   // expect: 1.5
print(float("-2.5"));  // expect: -2.5
print(float("0"));     // expect: 0
print(float(true));    // expect: 1
print(float(false));   // expect: 0
print(float(nil));     // expect: 0
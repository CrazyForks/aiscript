class Foo {
    fn init(a, b: str = "bb") {
        self.a = a;
        self.b = b;
    }
}

print(Foo(a="aa"));            // expect: Foo {b: bb, a: aa}
print(Foo("aa", b="bbb"));     // expect: Foo {b: bbb, a: aa}
print(Foo(a="aa", b="bbb"));   // expect: Foo {b: bbb, a: aa}
print(Foo(b="bbb", a="aa"));   // expect: Foo {b: bbb, a: aa}
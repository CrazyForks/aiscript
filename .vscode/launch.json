  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'aiscript-cli'",
      "cargo": {
        "args": ["build", "--bin=aiscript-cli", "--package=aiscript-cli"],
        "filter": {
          "name": "aiscript-cli",
          "kind": "bin"
        }
      },
      "args": ["main.ai"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
